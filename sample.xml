                <type>DateTime</type>
                <description>Date and time when the transaction occurred.</description>
            </field>
            <field>
                <name>merchant_id</name>
                <type>String</type>
                <description>Unique identifier for the merchant.</description>
            </field>
            <field>
                <name>product_category</name>
                <type>String</type>
                <description>Category of the product purchased.</description>
            </field>
            <field>
                <name>payment_details</name>
                <type>Nested</type>
                <description>Details about the payment method used.</description>
            </field>
            <field>
                <name>shipping_info</name>
                <type>Nested</type>
                <description>Details about the shipping address and method.</description>
            </field>
            <field>
                <name>customer_feedback</name>
                <type>Nested</type>
                <description>Feedback provided by the customer for the transaction.</description>
            </field>
        </field_metadata>
    </metadata>
